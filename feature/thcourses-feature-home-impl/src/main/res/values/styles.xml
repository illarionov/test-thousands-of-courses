<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.SearchBar" parent="TextAppearance.Material3.SearchBar">
        <item name="android:textColor">@color/thcoursesColorWhiteTranslucent50</item>
        <item name="android:textColorHint">@color/thcoursesColorWhiteTranslucent50</item>
        <item name="android:textSize">8sp</item>
    </style>

    <style name="ThemeOverlay.Thresources.SearchBar" parent="">
        <item name="colorOnSurfaceVariant">@color/thcoursesColorWhiteTranslucent50</item>
        <item name="navigationIconTint">@color/thcoursesColorWhite</item>
    </style>

    <style name="GreenTextButtonSmall">
        <item name="android:textAppearance">@style/TextAppearance.GreenTextIndicator</item>
        <item name="android:drawablePadding">4dp</item>
        <item name="android:gravity">end</item>
    </style>


    <style name="TextAppearance.GreenTextIndicator" parent="TextAppearance.Thresources.AndroidButton">
        <item name="android:textColor">@color/thcoursesColorGreen</item>
    </style>


</resources>
